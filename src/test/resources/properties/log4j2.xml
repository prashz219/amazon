<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<Appenders>
		<Console name="Console">
			<PatternLayout
				pattern="%d{HH:mm:ss:SSS} %-5level %c{1} -%msg%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="Console" />
		</Root>
	</Loggers>
</configuration>